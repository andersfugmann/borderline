# This zone defines all traffic to and from the machine itself,
# Along with the loopback device.
zone self {
     interface = lo;
     process filter {
         rule { source zone = int } policy log_allow;
         rule {
             destination address = 90.184.102.126;
             destination tcp port = 54323;
         } policy log_allow;
     } policy log_deny;
}
